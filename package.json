{
  "name": "code-splitting-example",
  "version": "1.0.0",
  "description": "A toy app demonstrating how to do code splitting in Webpack.",
  "main": "server.js",
  "scripts": {
    "build": "SET NODE_ENV=production && npx webpack",
    "start": "npm run build && SET NODE_ENV=production && node server.js",
    "dev": "NODE_ENV=development npx webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/malchata/code-splitting-example.git"
  },
  "keywords": [
    "webpack",
    "code",
    "splitting",
    "javascript"
  ],
  "author": "Jeremy L. Wagner <jeremy.l.wagner@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/malchata/code-splitting-example/issues"
  },
  "homepage": "https://github.com/malchata/code-splitting-example#readme",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.1",
    "babel-plugin-emotion": "^9.2.8",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "emotion": "^9.2.8",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "preact": "^8.3.1",
    "preact-emotion": "^9.2.8",
    "preact-router": "^2.6.1",
    "prop-types": "^15.6.2",
    "regenerator-runtime": "^0.12.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "modules": false
        }
      ]
    ],
    "plugins": [
      [
        "transform-react-jsx",
        {
          "pragma": "h"
        }
      ],
      "syntax-dynamic-import"
    ]
  }
}
